
  <!-- NDR6 rule 9-69 (same as NDR5 rule 10-27) -->
  <pattern id="rule9-69" xmlns="http://purl.oclc.org/dsdl/schematron">
    <title>An augmentation point element has no substitution group</title>
    <rule context="xs:element[exists(@name[
                                  matches(., 'AugmentationPoint$')])]">
      <assert test="empty(@substitutionGroup)"
        >Rule 9-69: An augmentation point element MUST have no substitution group.</assert>
    </rule>
  </pattern>
