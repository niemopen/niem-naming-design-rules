
  <!-- NDR6 rule 9-75 (same as NDR5 rule 9-80) -->
  <pattern id="rule9-75" xmlns="http://purl.oclc.org/dsdl/schematron">
    <title>Appinfo child elements have namespaces</title>
    <rule context="xs:appinfo/*">
      <assert test="namespace-uri() != xs:anyURI('')"
        >Rule 9-75: An element that is a child of xs:appinfo MUST have a namespace name.</assert>
    </rule>
  </pattern>
