  <!-- NDR6 rule 9-5 (same as NDR5 rule 9-72) -->
  <pattern id="rule9-5c" xmlns="http://purl.oclc.org/dsdl/schematron">
    <title>Prohibited schema components</title>
    <rule context="xs:unique">
      <assert test="false()"
        >Rule 9-5: A schema document MUST NOT contain xs:unique.</assert>
    </rule>
  </pattern>
