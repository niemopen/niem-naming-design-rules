  <!-- NDR6 rule 9-5 (same as NDR5 rule 9-61) -->
  <pattern id="rule9-5b" xmlns="http://purl.oclc.org/dsdl/schematron">
    <title>Prohibited schema components</title>
    <rule context="xs:all">
      <assert test="false()"
        >Rule 9-5: A schema document MUST NOT contain xs:all.</assert>
    </rule>
  </pattern>
