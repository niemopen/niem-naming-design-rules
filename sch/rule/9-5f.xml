  <!-- NDR6 rule 9-5 (same as NDR5 rule 9-75) -->
  <pattern id="rule9-5f" xmlns="http://purl.oclc.org/dsdl/schematron">
    <title>Prohibited schema components</title>
    <rule context="xs:group">
      <assert test="false()"
        >Rule 9-5: A schema document MUST NOT contain xs:group.</assert>
    </rule>
  </pattern>
