
  <!-- NDR6 rule 9-82 (same as NDR5 rule 9-35) -->
  <pattern id="rule9-82a" xmlns="http://purl.oclc.org/dsdl/schematron">
    <title>No disallowed derivation</title>
    <rule context="xs:complexType">
      <assert test="empty(@final)"
        >Rule 9-82: A reference schema document MUST NOT contain the attribute {}final or {}finalDefault.</assert>
    </rule>
  </pattern>
